<h2>Revisions</h2>

<script type="text/javascript">
function LimitSelect(elm) {
	if (!elm) return;
	if (sel1 == null) sel1 = elm;
	else if (sel2 == null) sel2 = elm;
	else if (sel1 == elm) sel1 = null;
	else if (sel2 == elm) sel2 = null;
	else {
		sel1.checked = false;
		sel1 = sel2;
		sel2 = elm;
	}

	return true;
}
</script>









