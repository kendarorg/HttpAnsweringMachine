[
  {
    "matchers": {
      "path": [
        "https://www.kendar.org(EXACT)"
        /* Exact */
      ],
      "query": [
        {
          "id": "p",
          "value": "!security(REGEXP, with trailing '!' )"
        }
      ],
      "body": {
        "bodyType": "xml|json|text|binary",
        "matchers": [
          {
            "path": "null(simply contains data)|json.data.fields[].field(any field)|json.data.fields[].field(any field)|xpath",
            "value": "null(check if field exists)|!value(REGEXP)|value(EXACT)",
          }
        ]
      }
    }
  }
]